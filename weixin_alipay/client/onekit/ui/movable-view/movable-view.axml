<movable-view
class="onekit-movable-view {{Class}}"
style="{{Style}}"
direction="{{direction}}"
inertia="{{inertia}}"
out-of-bounds="{{outOfBounds}}"
x="{{x}}"
y="{{y}}"
damping="{{damping}}"
friction="{{friction}}"
disabled="{{disabled}}"
scale="{{scale}}"
scale-min="{{scaleMin}}"
scale-max="{{scaleMax}}"
scale-value="{{scaleValue}}"
animation="{{animation}}"

onChange="movable_Change"
onScale="movable_scale"
>
<slot></slot>
</movable-view>