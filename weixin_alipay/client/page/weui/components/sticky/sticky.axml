<import-sjs name="computed" from="./sticky.wxs">
</import-sjs>


<view class="weui-sticky" style="{{computed.containerStyle({fixed, height, zIndex})}}">
  <view class="{{fixed ? 'weui-sticky__fixed' : ''}}" style="{{computed.wrapStyle({fixed, offsetTop})}}">
    <slot></slot>
  </view>
</view>