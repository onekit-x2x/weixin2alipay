<view class="page" data-weui-theme="{{theme}}">
    <view class="page__hd">
        <view class="page__title">Msg</view>
        <view class="page__desc">提示页</view>
    </view>
    <view class="page__bd">
        <view class="weui-btn-area">
            <onekit-button Class="weui-btn" type="default" onTap="openSuccess">成功提示页</onekit-button>
            <onekit-button Class="weui-btn" type="default" onTap="openFail">失败提示页</onekit-button>
            <onekit-button Class="weui-btn" type="default" onTap="openText">无图标提示页</onekit-button>
            <onekit-button Class="weui-btn" type="default" onTap="openTextPrimary">无图标提示页</onekit-button>
        </view>
    </view>
</view>